<h1>Edit Photo</h1>

<form action="/update_photo/<%= @id %>">
  <div>
    <label for="url">
      Image URL
    </label>
    <input id="url" type = "text" name="url" value="<%= @url %>"><br />
    <label for="cap">
      Caption
    </label>
    <input id="cap" type = "text" name="cap" value="<%= @cap %>">
  </div>
  <button>Update Photo</button>
</form>
