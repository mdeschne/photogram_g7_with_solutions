<div class="container">
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6"></div>
          <h1>All Photos</h1>
        <a class="btn btn-secondary" href="/photos/new">
          Add a new photo
        </a>
        <div class="col-sm-3"></div>
    </div>
</div>
<% @i.each do |entry| %>

<div class="container">
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6"></div>
          <img src = "<%= entry.source %>">
        <div class="col-sm-3"></div>
    </div>
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6"></div>
        <a class="btn btn-secondary btn-block" href="/photos/<%=entry.id%>">
        Show Details
        </a>
        <div class="col-sm-3"></div>
    </div>
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6"></div>
        <a class="btn btn-secondary btn-block" href="/photos/<%=entry.id%>/edit">
      Edit
        </a>
        <div class="col-sm-3"></div>
    </div>
    <div class="row">
        <div class="col-sm-3"></div>
        <div class="col-sm-6"></div>
        <a class="btn btn-secondary btn-block" href="/delete_photo/<%=entry.id%>">
          Delete
        </a>
        <div class="col-sm-3"></div>
    </div>


</div>







<% end %>
